block quick-start-billboard, elem logo {

    default: {
        var index = Math.round(10 * Math.random()),
            ctx = this.ctx,
            mod = ['batman', 'breaks', 'captain-america',
                    'generated', 'motion', 'robin',
                    'scheme', 'spots', 'stripe', 'waves'][index];

        ctx.mix || (ctx.mix = []);
        ctx.mix.push({ mods: { theme: mod }});

        return applyCtx(ctx);
    }
}

