block menu, this.data.menu, default: applyCtx(
    [
        this.data.menu.map(function(column, index) {
            return {
                block: 'menu-list',
                mods: { main: index === 0 },
                content: column
            }
        }),
        {
            block: 'page',
            elem: 'fullscreen'
        }
    ]
)
