block menu, this.data.menu, default: {

    return applyCtx([
        this.data.menu.map(function(column, index) {
            return {
                block: 'menu-list',
                content: column,
                mods: index === 0 ?
                    { type: 'main' } :
                    { type: 'default' }
            }
        }),
        this.data.menu.length > 1 ? {
            block: 'page',
            elem: 'fullscreen'
        } : ''
    ])
}
