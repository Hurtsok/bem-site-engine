block b-page, elem css {

    default: {
        var url = apply('asset', this.ctx.ext = this.elem),
            elem = this.elem,
            prefix = url.prefix,
            suffix = url.suffix;
        return [
            { elem : elem, url : prefix + '.' + suffix, ie : false },
            { elem : elem, url : prefix + '.ie9.' + suffix, ie : 'lte IE 9' }
        ];
    }

}