{
    "NODE_ENV": "production",
    "luster": {
        "workers": 4,
        "control": {
            "forkTimeout": 1000,
            "stopTimeout": 1000,
            "exitThreshold": 3000,
            "allowedSequentialDeaths": 3
        },
        "server": {
            "port": "",
            "groups": 1
        }
    },
    "update": {
        "enable": false,
        "cron": "0 */5 * * * *"
    },
    "logger": {
        "mode": "default",
        "level": "debug",
        "dir": "logs",
        "stdout": "",
        "stderr": ""
    },
    "statics": {
        "pathname": "/m"
    }
}
