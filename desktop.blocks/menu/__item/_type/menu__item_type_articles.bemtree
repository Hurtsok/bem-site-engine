block menu, elem item, elemMod type articles {

    content: {

        return [
            {
                block: 'post',
                mods: { view: 'menu-item' },
                type: this.ctx.type,
                id: this.ctx.id,
                category: this.ctx.category,
                active: this.elemMods['active']
            }
        ]
    }
