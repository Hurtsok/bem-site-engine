block bk-subject {

    content: [
        {
            elem : 'title',
            name : this.ctx.name,
            title : this.ctx.title
        },
        (this.ctx.description && {
                elem : 'description',
                content : this.ctx.description
            }),
        (this.ctx.examples && {
                elem : 'examples',
                content : this.ctx.examples
            }),
        this.ctx.content
    ]

    elem level {

        tag: ''

    }

}
