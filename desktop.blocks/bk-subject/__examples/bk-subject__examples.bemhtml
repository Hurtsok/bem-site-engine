block bk-subject, elem examples {

    default {

        !this._firstExample: {
            applyNext(this._firstExample = true)
        }

        (this.ctx.content && !this.ctx.content.length): false

    }

    content, this._firstExample: {
        elem : 'incut',
        content : BEM.I18N('bk-subject', 'examples')    // 'Примеры'
    }
    
    content: {
        var content = applyNext();
        return [content, this.ctx.content];
    }
    
}