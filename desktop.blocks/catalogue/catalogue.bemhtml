block catalogue {

    content: {
        elem : 'list',
        content : this.ctx.content
    }

    elem list-item {

        content: {
            block : 'topic',
            url : this.ctx.url,
            content : [
                {
                    elem : 'name',
                    content : this.ctx.name
                },
                {
                    elem : 'title',
                    content : this.ctx.title
                }
            ]
        }

    }

}