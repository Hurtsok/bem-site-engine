block post, elem title {

    content: [
        {
            block: 'link',
            url: this.ctx.category ?
            '/' + this.ctx.type + '/' + this.ctx.category + '/' + this.ctx.id :
            '/' + this.ctx.type + '/' + this.ctx.id,
            content: this.ctx.title
        }
    ]
}
