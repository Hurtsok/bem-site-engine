block posts, elem list {

    content {

        this._.isArray(this.ctx.source): {

            var self = this;

            return this.ctx.source.map(function(item){
                return {
                    block: 'post',
                    mods: self.mods,
                    source: item.id,
                    category: self.ctx.category
                }
            });
        }
    }
}
