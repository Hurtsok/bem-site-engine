block posts, elem list {

    content {

        this._.isArray(this.ctx.content): {
            var i = 0,
                buf = [],
                item;

            while(item = this.ctx.content[i]) {
                item.content = null;
                buf[i++] = {
                    block: 'posts-item',
                    params: this.ctx.params,
                    content: item
                }
            }

            return buf;
        }
    }
}
