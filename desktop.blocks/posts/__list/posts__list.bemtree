block posts, elem list {

    content {

        this._.isArray(this.ctx.source): {
            var buf = [];

            this.ctx.source.forEach(function(item){
                buf.push(apply(this._mode = '', this.ctx = {
                    block: 'post',
                    mods: this.mods,
                    source: item
                }));
            });

            return buf;
        }
    }
}
