block author, elem skype {

    default, !(this.ctx && this.ctx.content): {
        return false;
    }

    content: [
        {
            block: 'author',
            elem: 'label',
            content: ''
        },
        {
            block: 'author',
            elem: 'value',
            content: this.ctx.content
        }
    ]
}
