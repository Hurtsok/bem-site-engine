block b-page {

    default: {
        this.ctx.mods = { theme : 'normal', page : this.data.page };
        return applyCtx(this.ctx);
    }

    content: [
        {
            block : 'header',
            mix : { block : 'layout', mods : { type : 'serp' } }
        },
        { block : 'content' },
        { block : 'footer' }
    ]

}
